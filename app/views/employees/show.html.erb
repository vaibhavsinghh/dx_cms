<% @page_title = "Employees" %>
<%= link_to("<< Home", {:action => 'index'}, :class => 'back-link') %>
<div class="employees show">
	<h2>Employee details</h2>
	<table summary="Employee detail view">
		<tr>
			<th>EmpID</th>
			<td><%= @employee.emp_id %></td>
		</tr>
		<tr>
			<th>Name</th>
			<td><%= @employee.emp_name %></td>
		</tr>
		<tr>
			<th>Qualification</th>
			<td><%= @employee.qualification %></td>
		</tr>
		<tr>
			<th>DOB</th>
			<td><%= @employee.dob %></td>
		</tr>
		<tr>
			<th>DateOfJoining</th>
			<td><%= @employee.date_of_joining %></td>
		</tr>
		<tr>
			<th>YearsOfExperience</th>
			<td><%= @employee.exp_in_years %></td>
		</tr>
		<tr>
			<th>Created_at</th>
			<td><%= @employee.created_at %></td>
		</tr>
		<tr>
			<th>Updated_at</th>
			<td><%= @employee.updated_at %></td>
		</tr>
	</table>
</div>
	     
